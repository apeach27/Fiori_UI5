<mvc:View controllerName="sync.e05.odatamodelmultiupdate.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:t="sap.ui.table">
    <Page id="page" title="{i18n>title}">
        <content>
            <t:Table rows="{/CarrierSet}">
                <t:extension>
                    <!-- Toolbar -->
                    <Toolbar>
                        <ToolbarSpacer />
                        <Button icon="sap-icon://edit" press="onEdit" />
                    </Toolbar>
                </t:extension>
                <t:columns>
                    <!-- 컬럼 제목과 데이터 -->
                    <t:Column>
                        <Label text="{i18n>labelCarrid}" />
                        <t:template>
                            <ObjectIdentifier title="{Carrid}"/>
                        </t:template>
                    </t:Column>
                </t:columns>
            </t:Table>

        </content>
    </Page>
</mvc:View>
